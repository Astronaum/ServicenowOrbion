        <script><![CDATA[var RoleManagementScriptInclude = Class.create();
RoleManagementScriptInclude.prototype = Object.extendsObject(AbstractAjaxProcessor, {

    addRoleToUser: function() {
        var userId = this.getParameter('sys_id');
        var selectedRole = this.getParameter('role');
        var payload = JSON.parse(this.getParameter('payload'));

        var token = gs.getProperty('x_1539348_so.scim.api.token');
        if (!token) {
            return 'Authorization token is missing';
        }

        var url = 'https://earth-apis.my.evidian.com/scim/v2/api/Users/' + userId;
        
        var request = new sn_ws.RESTMessageV2();
        request.setHttpMethod('PATCH');
        request.setEndpoint(url);
        request.setRequestHeader('Authorization', 'Bearer ' + token);
        request.setRequestHeader('Content-Type', 'application/json');
        request.setRequestBody(JSON.stringify(payload));

        try {
            var response = request.execute();
            var responseBody = response.getBody();
            var httpResponseStatus = response.getStatusCode();
            
            if (httpResponseStatus >= 200 && httpResponseStatus < 300) {
                return 'success';
            } else {
                return 'error: ' + responseBody;
            }
        } catch (e) {
            return 'error: ' + e.message;
        }
    }
});
]]></script>
